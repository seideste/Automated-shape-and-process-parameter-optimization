FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       simpleFoam;

startFrom         latestTime; 

startTime         0;

stopAt            endTime;

endTime           10000;

deltaT            1;

writeControl      timeStep;

writeInterval     10000;

purgeWrite        0;

writeFormat       ascii;

writePrecision    10;

writeCompression  off;

timeFormat        general;

timePrecision     6;

runTimeModifiable true;


